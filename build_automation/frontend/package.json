{
  "name": "build-automation-frontend",
  "version": "1.0.0",
  "description": "Front-end for SolarSPELL build automation using ReactJS",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf static /S",
    "build": "npm run-script clean; webpack --mode=development",
    "build-prod": "npm run-script clean; webpack --mode=production",
    "build-bash": "(npm run clean 2> /dev/null || true) && webpack --mode=development",
    "build-bash-prod": "(npm run clean 2> /dev/null || true) && webpack --mode=production",
    "winclean": "rd /s /q static",
    "winbuild": "npm run-script winclean & webpack --mode=production",
    "winbuild-dev": "npm run-script winclean & webpack --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "School for the Future of Innovation in Society, Arizona State University.",
  "license": "MIT",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@date-io/moment": "^1.3.13",
    "@devexpress/dx-react-core": "^1.11.2",
    "@devexpress/dx-react-grid": "^1.11.2",
    "@devexpress/dx-react-grid-material-ui": "^1.11.2",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/styles": "^4.0.0",
    "axios": "^0.18.1",
    "chart.js": "^2.9.3",
    "csv": "^5.3.1",
    "date-fns": "^2.0.0-beta.5",
    "downshift": "^3.4.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "keycode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "papaparse": "^4.6.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-codemod": "^5.1.1",
    "react-dom": "^16.12.0",
    "react-sortable-tree": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  }
}
